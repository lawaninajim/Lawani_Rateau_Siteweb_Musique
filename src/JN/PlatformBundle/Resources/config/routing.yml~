
jn_platform_home:

    path:      /{page}

    defaults:

        _controller: JNPlatformBundle:Advert:index

        page:        1

    requirements:

        page: \d*

    

jn_platform_view:

    path:      /advert/{id}

    defaults:

        _controller: JNPlatformBundle:Advert:view

    requirements:

        id: \d+


jn_platform_add:

    path:      /add

    defaults:

        _controller: JNPlatformBundle:Advert:add


jn_platform_edit:

    path:      /edit/{id}

    defaults:

        _controller: JNPlatformBundle:Advert:edit

    requirements:

        id: \d+


jn_platform_delete:

    path:      /delete/{id}

    defaults:

        _controller: JNPlatformBundle:Advert:delete

    requirements:

        id: \d+

